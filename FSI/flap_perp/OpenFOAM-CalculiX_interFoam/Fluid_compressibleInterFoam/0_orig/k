FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1e-11;

boundaryField
{

    inlet
    {
        type            fixedValue;
        value           uniform 1e-05;
    }
    
    flap
    {
        type            fixedValue;
        value           uniform 1e-05;
    }
    
    upperWall
    {
        type            fixedValue;
        value           uniform 1e-05;
    }

    lowerWall
    {
        type            fixedValue;
        value           uniform 1e-11;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0.001;
        value           uniform 1e-11;
    }
    
    frontAndBack
    {
        type            empty;
    }
}

// ************************************************************************* //
