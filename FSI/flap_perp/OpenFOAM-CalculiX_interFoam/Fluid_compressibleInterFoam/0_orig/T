FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    flap
    {
        type            zeroGradient;
    }

    inlet
    {
//         type            fixedValue;
//         value           $internalField;
        type            zeroGradient;
    }
    
    upperWall
    {
        type            zeroGradient;
    }
    
    lowerWall
    {
        type            zeroGradient;
    }
    
    outlet
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}

